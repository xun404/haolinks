.label12 li span {color: #E0E0E0;display: block;padding: 0 6px;position: absolute;right: -13px;text-align: center;top: 0; width: 12px;}
.label12 .current {background: none repeat scroll 0 0 #4F90D0;color: #FFFFFF;font-weight: bold;height: 32px;z-index: 100;}
.label12 .current span {display: none;}
.table03 tr {background: url("http://img.ifeng.com/tres/finance/news/2012/0330/bg07.gif") repeat-x 0 -8px ;}
.table03 {line-height: 25px;text-align: center;}
.table03 td {line-height: 25px;}
.thTit01 th { color:#666;text-align: center; margin-bottom:10px;}
.thTit02 th , .thTit03 th{ background:#F8F9FB;border-bottom: 1px solid #E0E0E0;color:#666;text-align: center; margin-bottom:10px; border-top: 1px solid #E0E0E0;}
.thTit03 th { border-top:none;}

.bd {background: #FFFFFF; border: 1px solid #CCCCCC; border-top:none}

/*收藏精品*/
.collection{ }
.collection .bigPic{ float:left; width:239px; padding-right:21px; text-align:center; padding-left:10px}
.collection .bigPic img{ display:block; margin-bottom:12px}
.collection .smallPic{ float:right; width:729px; background:#fff; height:289px; overflow:hidden}
.collection .smallPic ul{ padding-top:12px}
.collection .smallPic ul li{ float:left; width:150px; line-height:32px; text-align:center; margin:0 11px 27px 19px; display:inline}
.collection .smallPic ul li img{ display:block}

/*财经封面秀*/
.cover{border:1px solid #cccccc;border-top:none; width:1000px; margin:0 auto}
.cover_tit{background:url(http://img.ifeng.com/tres/finance/deco/2011/0727/repeat_sprite.gif) repeat-x 0 -596px;height:48px;}
.cover_tit span{background:url(http://img.ifeng.com/tres/finance/deco/2011/0727/repeat_sprite.gif) repeat-x 0 -664px;height:28px;display:block;font-size:14px;padding:19px 0 0 10px;width:83px;}
.scrCover{padding:20px 0px 0 19px;}
.scrCover .btn04,.scrCover .btn05{float:left;display:inline;padding-top:59px;cursor:pointer;}
.scrCover .btn04{margin-right:20px;}
.scrCover .picList{float:left;display:inline;}
.picList li{float:left;display:inline;margin-right:20px;width:92px;text-align:center;}
.picList li img{border:1px solid #646263;display:block;margin-bottom:8px;}
.picList li h3{font-weight:normal;}
.scrollList_30 {float: left;height: 144px;overflow: hidden;position: relative;width: 896px;}
.scrollList_30 .photolist {position: relative;overflow: visible;width:10000px;}
.scrollList_30 .photolist ul{float:left;margin:0px;padding:0px;}
.scrollList_30 .photolist li {float:left;height:144px;}


/*友情链接*/
.cooper{border-top:7px solid #cccccc; width:1000px; margin:0 auto}
.cooper_L{float:left;width:748px;border-right:1px solid #cccccc;}
.cooper_R{float:left;width:250px;}
.tit11{border-bottom:1px solid #cccccc;font-size:14px;padding:12px 0 12px 10px;}
.media{padding:20px 0 0 11px;}
.media li{line-height:24px;}
.contact{padding:54px 0 0 34px;}
.contact p{line-height:24px;color:#666666;font-size:14px;margin-top:25px;}
.contact p span{color:#004276;}
