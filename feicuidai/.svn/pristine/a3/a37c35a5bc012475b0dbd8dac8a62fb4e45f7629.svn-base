<SCRIPT language=javascript>
		function CheckPage()
		{
			if (!CheckFN(document.all.edCarPrice,"请在车辆价格中输入正数",false))
	   			return false;
			//if (!CheckPN(document.all.edCarSeat,"请在座位数中输入正整数",false))	   			return false;
			if (!CheckFN(document.all.FeeRate,"请在保险费中输入非负数",false))
	   			return false;
			if (!CheckFN(document.all.TaxRate,"请在购置税中输入非负数",false))
	   			return false;
	   		if (!CheckFN(document.all.Road,"请在养路费中输入非负数",false))
	   			return false;
			if (!CheckFN(document.all.Justify,"请在公证费中输入非负数",false))
	   			return false;
			if (!CheckFN(document.all.Setax,"请在车船使用税中输入非负数",false))
	   			return false;
	   		if (!CheckFN(document.all.Tagfee,"请在上牌费中输入非负数",false))
	   			return false;
	   		return true;
		}
		function CalcMe()
		{
			if(!CheckPage())		   		
				return false;
	   		//CarPrice=parseInt(document.all.edCarPrice.value);	 		    
	   		//CarSeat=parseInt(document.all.edCarSeat.value); 
			calc(document);      
			return true;
		}
		
		function Change()
		{
			if(!CheckPage())		   		
				return false;			
			if (!CheckFN(document.all.Insur,"请在保险费中输入非负数",false))
	   			return false;
			if (!CheckFN(document.all.Extra,"请在购置税中输入非负数",false))
	   			return false;
	   		if (!CheckFN(document.all.Examcar,"请在验车费中输入非负数",false))
	   			return false;
			recalc(document); 		
		}
    </SCRIPT>

