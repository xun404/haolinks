@charset "utf-8";

.appointment_box{ width:100%; height:172px; background:#fff;}
.appointent{ width:1003px; height:auto; margin:0px auto;}
.appointent .guyingbao{ height:50px; border-bottom:1px solid #e70012; background:url(/resources/images/update/guyingbao.jpg) no-repeat 20px 20px; font-size:12px; text-align:right;}
.appointent .guyingbao span{ display:inline-block; margin-top:20px; padding-left:25px; background:url(/resources/images/update/clock.png) no-repeat left center; height:25px; line-height:25px; color:#c40f25;}
.appointent .Profit{ width:1003px; height:auto; background:url(/resources/images/update/tree.png) no-repeat 20px 20px;border-bottom:1px solid #ddd;}
.tree{ width:710px; float:left; height:auto; }

.tree .title{ height:70px; line-height:70px; text-indent:300px; font-size:16px;font-family:'微软雅黑'; color:#696969;}
.tree .title span{ color:#c40f25;font-size:18px;}
.tree .content{ width:570px; margin-left:140px;height:auto;min-height:auto;}
.tree .content ul{}
.tree .content ul li{ width:190px; text-indent:15px;line-height:50px; background:url(/resources/images/update/list.png) no-repeat 0px 20px; float:left;}
.tree .content ul li samp{color:#c40f25;}

.appointment{ width:270px; height:auto; float:right;}
.appointment a{ width:156px; height:45px; background:#c40f25; display:block; margin-left:60px; margin-top:37px;font-family:'微软雅黑'; font-size:18px; color:#fff; font-weight:bold; text-align:center; line-height:45px; border-radius:3px;}

.main{ width:1003px; height:auto; margin:20px auto;background:none; }
.main .left_area{ width:708px; height:auto; float:left;}
.whole{ width:708px; height:auto; background:#fff; margin-bottom:20px;border:1px solid #ddd; }
.whole .title{ height:55px;}
.title_itm{ width:625px; height:55px; float:left; }

.title_itm .icon{  width:60px; height:55px;float:left; padding-left:55px; line-height:55px; color:#c40f25; font-size:18px;font-family:'微软雅黑'; }
.title_itm .icon_1{ background:url(/resources/images/update/icon_1.png) no-repeat 10px 12px;}
.title_itm .icon_2{ background:url(/resources/images/update/icon_2.png) no-repeat 10px 12px;}
.title_itm .icon_3{ background:url(/resources/images/update/icon_3.png) no-repeat 10px 12px;}
.title_itm .icon_4{ background:url(/resources/images/update/icon_4.png) no-repeat 10px 12px;}
.title_itm .icon_5{ background:url(/resources/images/update/icon_5.png) no-repeat 10px 12px;}
.title_itm .icon_6{ background:url(/resources/images/update/icon_6.png) no-repeat 10px 12px;}



.title_itm .icon_a{ width:auto; height:55px; float:left;line-height:55px; padding-left:28px;overflow:hidden;}
.title_itm .icon_a .icon_span{ height:25px; padding:0px 8px 0px 0px; }

.title .more{ width:83px; height:35px; padding-top:20px; float:right;}
.title .more a{ padding:3px 10px; color:#fff; background:#ea5207; font-size:12px; border-radius:3px;}

.time_box{ width:100%; height:auto;background:url(/resources/images/update/time_bg.png) repeat-x left top;}
.time_box .time{ width:686px; height:45px; line-height:45px; margin:0px auto; border-bottom:1px solid #d7d7d7; overflow:hidden; }
.time_box .time .time_span{ padding:0px 8px; color:#ea5207;}

.loan_box{ width:100%; height:auto;}
.loan_box .loan{ width:686px; height:72px; margin:0px auto; border-bottom:1px solid #D7D7D7;}
.loan_box .last_loan{ border-bottom:none;}
.loan_left{ width:390px; height:72px; float:left;}
.loan_left .left_s{ height:36px; line-height:36px; font-size:14px;font-family:'微软雅黑';}
.loan_left .left_x{height:36px; line-height:36px;font-size:12px;}
.loan_left .left_x samp{ color:#ea5207; padding-right:30px;}
 
 .loan_right{ width:176px; height:72px;float:left;}
 .loan_right .right_s{height:24px;padding-top:12px;}
 
.DataLoadding{width:170px; height:15px; background:#ffc2a2; border-radius:10px; position:relative; text-align:center; line-height:15px; font-size:12px; color:#fff; z-index:1;}
.DataLoadding span{display:block; position:absolute; width:auto; height:15px; top:0px; left:0px; background:#f00;border-radius:10px; z-index:-2;}
.DataLoadding b{width:100%; font-size:13px; height:13px; line-height:13px; text-align:center; display:inline-block; color:#F17833; background:none; position:absolute; left:0px; top:0px; font-weight:normal;}


 .loan_right .right_x{height:36px; line-height:36px;}
 .loan_right .right_x samp{ color:#ea5207;}
 .result{ width:120px; height:72px; line-height:72px;float:left; }
 .result a{ display:block; width:85px; height:30px; line-height:30px;color:#fff;  margin:20px auto; text-align:center; border-radius:3px;font-family:'微软雅黑';}
 .result .bid{background:#ff8d55;}
 .result .bid:hover{ color:#fff;}
 .result .completed{background:#f00;}
  .result .completed:hover{color:#fff;}
 .result .full{ background:#ea5207;}
  .result .full:hover{color:#fff;}
 .result .repayment{ background:#afafaf;}
 .chick_more{ display:block; height:45px; width:100%; line-height:45px; text-align:center;}



.main .right_area{ width:270px; height:auto; float:right;}

.list_box{ width:270px; height:auto; margin-bottom:18px; background:#fff;border:1px solid #ddd; }
.list_box h3{ height:38px; background:url(/resources/images/update/h3_bg.jpg) no-repeat left center; color:#fff;}
.list_box h3 .h3_left{ width:174px; height:38px; line-height:38px; float:left; padding-left:50px;font-family:'微软雅黑'; font-size:16px;font-weight:400;}

.list_box h3 .left_01{ background:url(/resources/images/update/h3_icon_1.png) no-repeat 20px 10px; }
.list_box h3 .left_02{ background:url(/resources/images/update/h3_icon_2.png) no-repeat 20px 10px; }
.list_box h3 .left_03{ background:url(/resources/images/update/h3_icon_3.png) no-repeat 20px 10px; }
.list_box h3 .left_04{ background:url(/resources/images/update/h3_icon_4.png) no-repeat 20px 10px; }

.list_box h3 .h3_right{ width:46px; height:38px; line-height:38px;float:left; text-align:center; font-size:14px; color:#fff;}
.list_box h3 .h3_right a{ color:#fff; font-weight:400; }

.list_box ul{ width:270px; height:auto;  margin-top:2px; padding-bottom:2px;}
.list_box ul hr{ width:270px; border-top:2px solid #c40f25;}

.advertisement{ height:94px; padding-top:4px; text-align:center;}

.list_box ul li{ width:230px; height:auto; margin:0px auto; background:url(/resources/images/update/ring.png) no-repeat 5px 10px; line-height:30px; padding-left:20px;border-bottom:1px dashed #a8a8a8;}
.list_box ul li:nth-last-child(){ border-bottom:none;}
.list_box ul li a{ display:block; }

.partners_box{ width:100%; height:134px; background:#fff; padding:20px 0px;}
.partners{ width:1003px; height:94px; margin:20px auto; border:1px solid #ddd; position:relative;}
.partner{ width:49px; height:94px; position:absolute; left:0px; top:0px; background:url(/resources/images/update/partner_left_bg.png) no-repeat left center; border:none; }
.partners ul{ width:940px; margin-left:63px;}
.partners ul li{ width:auto; height:70px; float:left; padding-top:24px; padding-right:60px;}
.partners ul li a{ display:block; text-align:center;} 

/*.foot{ width:100%; height:auto; background:#475058;}*/
.foot .foot_box{ width:1003px; height:auto; margin:0px auto; padding:30px 0px;}
