.foot_box .foot_right_box{ width:517px; height:auto; float:right;}
.foot_right_box .service{height:46px;  border-radius:3px; background:url(/resources/images/update/telp.jpg) no-repeat left top; margin-top:15px; padding-top:80px; padding-left:20px; }

.foot_right_box .service span{ width:190px; height:32px; display:block; }
.foot_right_box .service span a{ display:inline-block; width:32px; height:32px; }

.foot_right_box .service span .ico_1{ background:url(/resources/images/update/communication.png) no-repeat left center;}
.foot_right_box .service span .ico_2{ background:url(/resources/images/update/communication.png) no-repeat -38px -1px;}
.foot_right_box .service span .ico_3{ background:url(/resources/images/update/communication.png) no-repeat -75px -1px;}
.foot_right_box .service span .ico_4{ background:url(/resources/images/update/communication.png) no-repeat -112px -1px;}
.foot_right_box .service span .ico_5{ background:url(/resources/images/update/communication.png) no-repeat -150px -1px;}

.foot_right_box .service span .ico_1:hover{ background:url(/resources/images/update/wxtb.png) no-repeat left center;}
.foot_right_box .service span .ico_2:hover{ background:url(/resources/images/update/wxtb.png) no-repeat -38px -1px;}
.foot_right_box .service span .ico_3:hover{ background:url(/resources/images/update/wxtb.png) no-repeat -75px -1px;}
.foot_right_box .service span .ico_4:hover{ background:url(/resources/images/update/wxtb.png) no-repeat -112px -1px;}
.foot_right_box .service span .ico_5:hover{ background:url(/resources/images/update/wxtb.png) no-repeat -150px -1px;}

.foot_right_box .service span a:hover{}

