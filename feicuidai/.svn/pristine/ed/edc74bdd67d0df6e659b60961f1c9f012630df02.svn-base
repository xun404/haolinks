.bottom .bottom_box .bottom_img a{ display:inline-block; height:30px; margin-right:10px;}
.bottom_img .trusted_01{ background:url(/resources/images/update/trusted_sites.png) no-repeat 0px 0px; width:102px; height:30px;}
