.rollBox {
	width: 1000px;
	height: 260px;
	overflow: hidden;
	margin: 0 auto;
	padding: 20px 0px;
}

.rollBox .LeftBotton {
	height: 40px;
	width: 22px;
	background: url(/resource/images/left_arrow.png) no-repeat 10px 5px;
	overflow: hidden;
	float: left;
	display: inline;
	margin: 110px 0 0 0;
	cursor: pointer;
}

.rollBox .RightBotton {
	height: 40px;
	width: 22px;
	background: url(/resource/images/right_arrow.png) no-repeat 0px 5px;
	overflow: hidden;
	float: left;
	display: inline;
	margin: 110px 0 0 0;
	cursor: pointer;
}

.rollBox .Cont {
	width: 955px;
	overflow: hidden;
	float: left;
}

.rollBox .ScrCont {
	width: 10000000px;
}

.rollBox .Cont .pic {
	width: 242px;
	float: left;
	text-align: center;
}

.rollBox .Cont .pic img {
	padding: 9px;
	background: #fff;
	border: 1px solid #ccc;
	display: block;
	margin: 0 auto;
	width: 190px;
	height: 190px;
}

.rollBox .Cont .pic div {
	line-height: 20px;
	color: #505050;
	margin: 15px 0px;
}

.rollBox .Cont .pic div span {
	display: block;
}

.rollBox .Cont a:link,.rollBox .Cont a:visited {
	color: #000;
	font-size:16px;
	text-decoration: none;
}

.rollBox .Cont a:hover {
	color: #f00;
	text-decoration: underline;
}

.rollBox #List1,.rollBox #List2 {
	float: left;
}