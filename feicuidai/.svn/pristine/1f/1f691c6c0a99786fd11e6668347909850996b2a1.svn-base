* {
	margin: 0px;
	padding: 0px;
}

.bank_ka{ border:1px solid #ACACAC; width:330px; height:50px; float:left; margin-bottom:50px; position:relative; }
 .select_box
{
	width:330px;
	height:50px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	text-align:left;

}

.tag_select
{
	width:330px;
	height:50px;
	line-height:50px;
	background: url(/resource/images/down_arrow.jpg)no-repeat right center;
	cursor: pointer;
	text-align:left;
}

.tag_select_open
{
	line-height:50px;
	cursor: pointer;
	background: url(/resource/images/up_arrow.jpg)no-repeat right center;
}

.tag_options{
	position:absolute;
	list-style:none;
	width:330px;
	height:auto;
	text-align: left;
	z-index: 999;
	display:none;
	border:1px solid #acacac;
}

.tag_options li
{
    width: 330px;
    height: 50px;
    line-height: 50px;
    cursor: pointer;
	background:#fff;
	text-align:left;
}
