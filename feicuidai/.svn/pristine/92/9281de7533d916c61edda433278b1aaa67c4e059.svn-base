
.login_box {
    width: 1000px;
    height: 485px;
    border-radius: 5px;
    border: 2px solid #e9e9e9;
    margin: 20px auto;
    margin-top: -50px;
    background: #fff;
}

.login_box .main {
    width: 620px;
    height: 306px;
    margin: 70px auto;
}

.login_box .main .title {
    height: 45px;
    background: url(/resource/images/login_title.png) no-repeat 0px 10px;
    text-indent: 40px;
    line-height: 45px;
    font-size: 18px;
    color: #00bc00;
}

.content_box {
    width: 620px;
    height: auto;
    margin: 0px auto;
}

.login_left_box {
    width: 332px;
    height: auto;
    float: left;
}

.login_left_box .input_box {
    height: 60px;
}

.login_left_box .input_box .input_style {
    width: 330px;
    height: 40px;
    line-height: 40px;
    border: none;
    border: 1px solid #acacac;
    border-radius: 5px;
    text-indent: 45px;
    color: #acacac;
    font-family: "微软雅黑";
}

.login_left_box .input_box .username {
    background: url(/resource/images/user.png) no-repeat 10px 12px;
}

.login_left_box .input_box .parssword {
    background: url(/resource/images/parssword.png) no-repeat 10px 10px;
}

.login_left_box .text_box .verification {
    background: url(/resource/images/seach_yanzheng.png) no-repeat 10px 10px;
    text-indent: 45px;
    color: #ACACAC;
}

.verification_box {
    height: 60px;
}

.verification_box .text_box {
    width: 230px;
    height: 40px;
    line-height: 40px;
    float: left;
}

.verification_box .text_box .text {
    border: none;
    border: 1px solid #acacac;
    border-radius: 5px;
    width: 210px;
    height: 40px;
    line-height: 40px;
	font-family:'微软雅黑';
}

.verification_box .pic_box {
    width: 102px;
    height: 40px;
    float: right;
}

.verification_bos {
    height: 40px;
}

.verification_box .remember {
    line-height: 12px;
    color: #7f7f7f;
}

.verification_box .pic_box {
    line-height: 12px;
}

.verification_box .pic_box a {
    color: #fb4646;
}

.input_button_box {
    height: 40px;
}

.login_left_box .input_button_box .button {
    width: 330px;
    height: 40px;
    cursor: pointer;
    line-height: 35px;
    border: none;
    border: 1px solid #acacac;
    border-radius: 5px;
    text-align: center;
    background: #00bc00;
    font-size: 17px;
    color: #fff;
    font-family: "微软雅黑";
}

.login_right_box {
    width: 230px;
    height: 260px;
    float: right;
    background: #efefef;
    color: #5f5e5e;
}

.login_right_box .tex_01 {
    height: 30px;
    padding-top: 50px;
    text-align: center;
}

.login_right_box .tex_01 a {
    color: #fb4646;
}

.login_right_box .tex_02 {
    height: 50px;
    line-height: 50px;
    text-align: center;
}

.login_right_box .login_icon {
    height: 50px;
    width:auto;
    margin: 20px auto;
    padding-left:50px;
}

.login_right_box .login_icon a {
    padding: 15px 29px;
}

.login_right_box .login_icon .ion_1 {
    background: url(/resource/images/login_icon.png) no-repeat 0px 0px;
}

.login_right_box .login_icon .ion_2 {
    padding: 15px 30px;
    background: url(/resource/images/login_icon.png) no-repeat -50px 0px;
}

.login_right_box .login_icon .ion_3 {
    background: url(/resource/images/login_icon.png) no-repeat -117px 0px;
}
