.sideToolBar .appDownload {
    background-position: 11px -440px;
    animation: appDownload 5s linear infinite;
    -webkit-animation: appDownload 5s linear infinite;
    -moz-animation: appDownload 5s linear infinite;
    -ms-animation: appDownload 5s linear infinite;
    -o-animation: appDownload 5s linear infinite
}

.sideToolBar .appDownload:hover {
    background-position: 11px -476px;
    animation: none;
    -webkit-animation: none;
    -moz-animation: none;
    -ms-animation: none;
    -o-animation: none
}

.sideToolBar .qqService {
    background-position: 7px -512px;
    animation: qqService 5s linear infinite;
    -webkit-animation: qqService 5s linear infinite;
    -moz-animation: qqService 5s linear infinite;
    -ms-animation: qqService 5s linear infinite;
    -o-animation: qqService 5s linear infinite
}

.sideToolBar .qqService:hover {
    background-position: 7px -548px;
    animation: none;
    -webkit-animation: none;
    -moz-animation: none;
    -ms-animation: none;
    -o-animation: none
}

.sideToolBar .wechatQRCode {
    cursor: default;
    background-position: 7px -584px
}

.sideToolBar .wechatQRCode:hover {
    background-position: 7px -620px
}

.sideToolBar .returnTop {
    display: none;
    background-position: 7px -656px
}

.sideToolBar .returnTop:hover {
    background-position: 7px -692px
}

.sideToolBar a i {
    width: 0;
    height: 36px;
    line-height: 36px;
    overflow: hidden;
    position: absolute;
    right: 36px;
    top: 0;
    text-align: center;
    opacity: 0;
    filter: alpha(opacity=0)
}

.sideToolBar .qqService i {
    top: 46px
}

.sideToolBar .returnTop i {
    top: 138px
}

.sideToolBar .wechatQRCode i {
    height: 181px;
    position: absolute;
    top: -37px;
    background: url(/resource/images/qqwei-v2.png) no-repeat
}

.sideToolBar a:hover i {
    width: 88px;
    opacity: 1;
    filter: alpha(opacity=100)
}

.sideToolBar .wechatQRCode:hover i {
    width: 162px
}

.bubble {
    display: block;
    position: absolute;
    z-index: 10;
    left: 83px;
    top: 10px;
    color: #fff;
    font-size: 12px;
    width: 21px;
    height: 16px;
    line-height: 16px;
    vertical-align: middle;
    text-align: center;
    background: url(/resource/images/bubble.png) no-repeat
}