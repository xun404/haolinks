@charset "utf-8";
blockquote, body, dd, div, dl, dt, fieldset, form, h1, h2, h3, h4, h5, h6, img, input, legend, li, ol, p, pre, select, table, textarea, ul {
    margin: 0;
    padding: 0
}

body {
    font: 14px/1.5 "Microsoft YaHei", "Hiragino Sans GB", arial, tahoma;
    background: #fff
}

a, body {
    color: #333
}

a {
    text-decoration: none
}

.sideToolBar a, .sideToolBar a i, .smartTender .intro, .textList1 li a, .textList1 li a:before, .videoSlide > .num b, .videoSlide > em {
    transition: all .2s linear;
    -webkit-transition: all .2s linear;
    -moz-transition: all .2s linear;
    -ms-transition: all .2s linear;
    -o-transition: all .2s linear
}

.footerNav .iconBox a, .footerNav .wrap > .fr .serviceLink, .notice .speaker, .sideToolBar a, .topNav .loginBar i {
    background: url(/resource/images/icon-sprite-v2-2.png) 0 6px no-repeat
}

.topNav {
    line-height: 40px;
    background: #666
}

.topNav, .topNav a {
    color: #d6d6d6
}

.topNav a:hover {
    color: #fff
}

.topNav .fl b {
    margin: 0 12px 0 5px
}

.topNav .loginBar .signUp {
    color: #6c6
}

.topNav .loginBar .signUp:hover {
    color: #6ce56c
}

.topNav .loginBar .signIn {
    margin-left: 20px;
    color: #00a4ff
}

.topNav .loginBar .signIn:hover {
    color: #2bf
}

.topNav .loginBar i {
    padding-left: 24px;
    background-position: 0 -31px
}

.topNav .loginBar b {
    color: #ffd400
}

.sideToolBar {
    width: 36px;
    position: fixed;
    right: 5px;
    top:44%;
    z-index:6666;
}

.sideToolBar a {
    height: 36px;
    margin-bottom: 10px;
    display: block;
    background-color: #e5e5e5
}

.sideToolBar a i, .sideToolBar a:hover {
    color: #fff;
    background-color: #309f29
}

