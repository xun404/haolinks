function checkbox_init(e){e.checkboxs=new HashMap}function checkbox_click_all(e,t,n){n?$.each(t.find("."+e.classTr+" input:checkbox"),function(t,n){$(n).attr("checked")!=1&&(checkbox_click(e,$(n),!0),$(n).attr("checked",!0))}):$.each(t.find("."+e.classTr+" input:checkbox"),function(t,n){$(n).attr("checked")!=0&&(checkbox_click(e,$(n),!1),$(n).attr("checked",!1))})}function checkbox_clear(e,t){$.each(t.find("."+e.classTr+" input:checkbox"),function(t,n){$(n).attr("checked")!=0&&(checkbox_click(e,$(n),!1),$(n).attr("checked",!1))}),t.find("#chk_all").attr("checked",!1),e.checkboxs.removeAll(),update_checkboxs_size(e)}function fit_checkbox(e,t,n){t.append('<td><input type="checkbox" name="item" class="item_" value="'+n[e.primary]+'" /></td>');var r=t.children("td:first").children();r.bind("click",function(){checkbox_click(e,$(this),$(this).attr("checked")),checkbox_is_all(e)}),callback_checkbox(e,r)}function checkbox_click(e,t,n){n==1?(checkbox_put(e,t.val()),t.parent().parent().addClass(class_checkboxed)):(checkbox_remove(e,t.val()),t.parent().parent().removeClass(class_checkboxed))}function checkbox_put(e,t){e.checkboxs.put(t,null),update_checkboxs_size(e)}function checkbox_checkeds(e){return $("#"+e).data().checkboxs.keySet()}function checkbox_delete(e,t){checkbox_remove($("#"+e).data(),t)}function checkbox_delete_all(e){var t=$("#"+e).data();checkbox_clear(t,$("#"+t.tag).find("#"+t.idTable))}function checkbox_remove(e,t){e.checkboxs.remove(t),update_checkboxs_size(e)}function checkbox_is_all(e){var t=$("#"+e.tag).find("#"+e.idTable),n=t.find("."+e.classTr);n.find("input:checked").size()==n.find("input:checkbox").size()?t.find("#chk_all").attr("checked",!0):t.find("#chk_all").attr("checked",!1)}function update_checkboxs_size(e){var t=$("#"+e.tag).find("#"+e.idTable),n=e.checkboxs.size();t.find("#span_checkboxs_size").text(n),e.checkbox.bottomTip=="auto"&&(n>0?t.find(".end").fadeIn(200):t.find(".end").fadeOut(200))}var class_checkboxed="light_bar_checked"