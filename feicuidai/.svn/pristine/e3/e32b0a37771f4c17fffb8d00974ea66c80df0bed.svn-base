.input_box {
	height: 72px;
	margin: 0px auto;
	width: 500px;
}

.input_box .content_input {
	height: 40px;
	line-height: 40px;
	color: #7f7f7f;
}

.input_box .content_input span {
	display: inline-block;
	width: 85px;
	letter-spacing: 2px;
	text-align: justify;
	text-justify: inter-ideograph;
}

.input_box .content_input span samp {
	color: #f00;
	padding-right: 5px;
}

.input_text_style {
	padding-left: 2px;
	width: 250px;
	font-family: "微软雅黑";
	padding: 10px 0px;
	text-indent: 10px;
	border: none;
	border: 1px solid #acacac;
	border-radius: 5px;
}

.input_box .color_red {
	color: #fb4646;
	text-align: left;
	padding-left: 10px;
	line-height: 30px;
}

.input_box select {
	padding: 10px 23px;
	border: none;
	border-radius: 5px;
	border: 1px solid #acacac;
	color: #7f7f7f;
}

.input_box .button_style {
	border: none;
	width: 250px;
	margin: 20px 0px;
	font-weight: bold;
	height: 35px;
	line-height: 35px;
	text-align: center;
	background: #00bc00;
	color: #fff;
	font-size: 17px;
	border-radius: 5px;
	cursor: pointer;
}

.input_box .button_style:hover {
	background: #1a9d1a;
}

