/*-------弹出框--------*/
.zhezhaocheng {
	background: #000;
	position: fixed;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	z-index: 999;
	filter: alpha(opacity = 60);
	-moz-opacity: 0.6;
	opacity: 0.6;
}

.pop_box {
	width: 648px;
	height: auto;
	border: 1px solid #ddd;
	display: none;
	position: fixed;
	top: 28%;
	left: 32%;
	z-index: 10000;
	background: #FFF
}

.pop_box .pop {
	width: 592px;
	height: 43px;
	padding-top: 15px;
	margin: 0px auto;
	background: url(/resource/images/security_validation.png) no-repeat left
	center;
	text-align: right;
}

.pop_box .pop_bank {
	background: url(/resource/images/queren_bank.png) no-repeat left center;
}

.pop_box .lica {
	background: url(/resource/images/licai_jisuan.png) no-repeat left center;
}

