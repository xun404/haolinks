<SCRIPT language=javascript>
		function CheckData()
		{
			if (!CheckFN3(this.document.all.tbIncomeBeforeTax,"税前收入请输入正数",false))
				return false;
			if (this.document.all.ddlIncomeType.selectedIndex==0)
					{				
						if (!CheckFN3(this.document.all.tbAllInsureFee,"各项社会保险费请输入非负数",true))
							return false;
						if (!CheckPN(this.document.all.tbThresholdSum,"起征额请输入正整数",false))
							return false;
					}
			return true;
		}							
		</SCRIPT>
